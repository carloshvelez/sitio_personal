{
    "builds": [
        {
            "src": "sitio_personal/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "staticfiles/(.*)",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "sitio_personal/wsgi.py"
        }
    ]
}
