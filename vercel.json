
{
    "builds": [{        
        "use": "@vercel/python",
        "src": "sitio_personal/wsgi.py",
        "config": {
            "maxLambdaSize": "15mb", "runtime": "python3.13.1"
        }
    }],
    "routes": [        
        {
            "src": "/(.*)",
            "dest": "sitio_personal/wsgi.py"
        }
    ]
}